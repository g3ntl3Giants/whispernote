{
  "name": "whispernote",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "android:stacktrace": "cd android && ./gradlew app:installDebug -PreactNativeDevServerPort=8081 --stacktrace && cd ../",
    "android:debug": "cd android && ./gradlew app:installDebug -PreactNativeDevServerPort=8081 --debug && cd ../",
    "ios": "npx react-native run-ios",
    "macos": "npx react-native run-macos",
    "apple-pod-install": "cd ios && pod install && cd ../macos && pod install && cd ../",
    "lint": "eslint .",
    "start": "APP_ENV=development npx react-native start --reset-cache",
    "start:test": "APP_ENV=test npx react-native start --reset-cache",
    "test": "jest",
    "nuke:node": "rm -rf node_modules && rm -rf package-lock.json yarn.lock && yarn install",
    "nuke:ios": "cd ios && rm -rf Pods && rm -rf Podfile.lock && rm -rf build && rm -rf ~/Library/Developer/Xcode/DerivedData/WhisperNote-* && RCT_NEW_ARCH_ENABLED=1 bundle exec pod install && cd ../",
    "nuke:android": "cd android && ./gradlew clean && cd ../",
    "nuke:macos": "cd macos && rm -rf Pods && rm -rf Podfile.lock && rm -rf build && rm -rf ~/Library/Developer/Xcode/DerivedData/WhisperNote-* && pod install && cd ../"
  },
  "dependencies": {
    "@react-native/gradle-plugin": "^0.72.10",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/stack": "^6.3.16",
    "axios": "^1.4.0",
    "form-data": "^4.0.0",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-audio-recorder-player": "^3.5.3",
    "react-native-document-picker": "^9.0.1",
    "react-native-gesture-handler": "^2.10.1",
    "react-native-gradle-plugin": "^0.71.18",
    "react-native-network-logger": "^1.15.0",
    "react-native-safe-area-context": "^4.5.3",
    "react-native-screens": "^3.20.0",
    "react-native-svg": "^13.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@testing-library/react-native": "^12.1.2",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "jest-native": "^3.0.0",
    "metro-react-native-babel-preset": "0.73.9",
    "prettier": "^2.4.1",
    "react-native-codegen": "^0.71.5",
    "react-native-dotenv": "^3.4.8",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
